    "dependencies": {
        "css-chain": "1.1.7",
        "lit": "2.8.0"
    },
    "devDependencies": {
        "@open-wc/testing": "3.2.0",
        "@web/dev-server": "0.3.0",
        "@web/test-runner": "0.17.0",
        "concurrently": "8.2.0",
        "esbuild": "0.18.17",
        "typescript": "5.1.6"
    },
    "customElements": "custom-elements.json"
}
